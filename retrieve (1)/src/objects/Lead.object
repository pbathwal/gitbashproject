<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>No_Email_or_Phone</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISBLANK(MobilePhone),
ISBLANK( Email )
)</errorConditionFormula>
        <errorDisplayField>Email</errorDisplayField>
        <errorMessage>Please provide either email or mobile</errorMessage>
    </validationRules>
</CustomObject>
